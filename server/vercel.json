{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"version": 2,
	"regions": ["bom1"],
	"buildCommand": "npm run build",
	"builds": [
		{
			"src": "dist/index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "dist/index.js",
			"methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
			"headers": {
				"Access-Control-Allow-Origin": "*"
			}
		}
	]
}