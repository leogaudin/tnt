{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"version": 2,
	"regions": ["bom1"],
	"functions": {
		"index.ts": {
			"runtime": "@vercel/node@3"
		}
	},
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/index.ts"
		}
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{ "key": "Access-Control-Allow-Origin", "value": "*" },
				{ "key": "Access-Control-Allow-Methods", "value": "GET,POST,PUT,DELETE,PATCH,OPTIONS" }
			]
		}
	]
}